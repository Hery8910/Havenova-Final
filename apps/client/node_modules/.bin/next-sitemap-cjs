#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/heriberto/Escritorio/Havenova/havenova/node_modules/.pnpm/next-sitemap@4.2.3_next@14.2.32_@babel+core@7.28.4_react-dom@18.3.1_react@18.3.1__react@18.3.1_/node_modules/next-sitemap/bin/node_modules:/home/heriberto/Escritorio/Havenova/havenova/node_modules/.pnpm/next-sitemap@4.2.3_next@14.2.32_@babel+core@7.28.4_react-dom@18.3.1_react@18.3.1__react@18.3.1_/node_modules/next-sitemap/node_modules:/home/heriberto/Escritorio/Havenova/havenova/node_modules/.pnpm/next-sitemap@4.2.3_next@14.2.32_@babel+core@7.28.4_react-dom@18.3.1_react@18.3.1__react@18.3.1_/node_modules:/home/heriberto/Escritorio/Havenova/havenova/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/heriberto/Escritorio/Havenova/havenova/node_modules/.pnpm/next-sitemap@4.2.3_next@14.2.32_@babel+core@7.28.4_react-dom@18.3.1_react@18.3.1__react@18.3.1_/node_modules/next-sitemap/bin/node_modules:/home/heriberto/Escritorio/Havenova/havenova/node_modules/.pnpm/next-sitemap@4.2.3_next@14.2.32_@babel+core@7.28.4_react-dom@18.3.1_react@18.3.1__react@18.3.1_/node_modules/next-sitemap/node_modules:/home/heriberto/Escritorio/Havenova/havenova/node_modules/.pnpm/next-sitemap@4.2.3_next@14.2.32_@babel+core@7.28.4_react-dom@18.3.1_react@18.3.1__react@18.3.1_/node_modules:/home/heriberto/Escritorio/Havenova/havenova/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../next-sitemap/bin/next-sitemap.cjs" "$@"
else
  exec node  "$basedir/../next-sitemap/bin/next-sitemap.cjs" "$@"
fi
